{
  "extends": "./tsconfig.base.json",
  "compilerOptions": {
    "tsBuildInfoFile": ".next/cache/tsconfig.tsbuildinfo",
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "src/subapps/dxf-viewer/types/dxf-modules.d.ts",
    ".next/dev/types/**/*.ts"
  ],
  "exclude": [
    "node_modules",
    ".next",
    "-.next",
    "functions/**",
    "src/subapps/dxf-viewer/patches/**",
    "src/subapps/dxf-viewer/archive/**",
    "src/subapps/dxf-viewer-10-backup/**",
    "src/app/test.BACK/**",
    "src/subapps/dxf-viewer/test.BACK/**",
    "src/ai/**",
    "src/ai.back/**",
    "DxfViewerApp.tsx",
    "recovery/**"
  ]
}
