    "eslint-plugin-unused-imports": "^4.1.4",
    "jest": "^30.2.0",
    "jest-axe": "^10.0.0",
    "jest-environment-jsdom": "^30.2.0",
    "jsdom": "^27.0.0",
    "picocolors": "1.1.1",
