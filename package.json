{
  "name": "nextn",
  "version": "0.1.1",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:fast": "next dev --turbo",
    "dev:clean": "npm run clear-cache && next dev",
    "build": "next build",
    "build:clean": "node scripts/clear-cache.js && npm run build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "lint:strict": "npx eslint . --ext .js,.mjs,.ts,.tsx,.jsx --max-warnings 0",
    "typecheck": "tsc --noEmit",
    "typecheck:strict": "tsc --noEmit --strict",
    "strict-mode": "node scripts/simple-strict-check.js",
    "clear-cache": "node scripts/clear-cache.js",
    "postinstall": "patch-package",
    "seed:units": "tsx --require dotenv/config scripts/seedUnits.ts",
    "seed:projects": "tsx --require dotenv/config scripts/seedProjects.ts",
    "seed:buildings": "tsx --require dotenv/config scripts/seedBuildings.ts",
    "seed:contacts": "tsx --require dotenv/config scripts/seedContacts.ts",
    "seed:storage": "tsx --require dotenv/config scripts/seedStorage.ts",
    "seed:opportunities": "tsx --require dotenv/config src/data/seed/seed-opportunities.ts",
    "seed:parking": "tsx --require dotenv/config scripts/seedParking.ts",
    "seed:firestore": "tsx --require dotenv/config src/data/seed/seed-firestore.ts",
    "seed:communications": "tsx --require dotenv/config src/data/seed/seed-communications.ts",
    "seed:tasks": "tsx --require dotenv/config src/data/seed/seed-tasks.ts",
    "seed:all": "npm run seed:projects && npm run seed:buildings && npm run seed:contacts && npm run seed:units && npm run seed:storage && npm run seed:opportunities && npm run seed:parking && npm run seed:firestore && npm run seed:communications && npm run seed:tasks",
    "generate:i18n-types": "node scripts/generate-i18n-types.js",
    "extract:strings": "node scripts/extract-strings.js",
    "validate:i18n": "node scripts/validate-translations.js",
    "validate:i18n-config": "node scripts/validate-i18n-config.js",
    "extract:hardcoded": "node scripts/extract-hardcoded-strings.js",
    "i18n:check": "npm run validate:i18n && npm run extract:hardcoded",
    "i18n:migrate": "node scripts/i18n-migration-helper.js",
    "i18n:migrate:file": "node scripts/i18n-migration-helper.js",
    "analyze:bundle": "node scripts/bundle-analyzer.js",
    "analyze:bundle-report": "npm run analyze:bundle && echo 'Report saved to bundle-analysis-report.json'",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --maxWorkers=2",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:visual": "playwright test e2e/grid-visual-regression.spec.ts",
    "test:visual:update": "playwright test e2e/grid-visual-regression.spec.ts --update-snapshots",
    "test:visual:headed": "playwright test e2e/grid-visual-regression.spec.ts --headed",
    "test:visual:report": "playwright show-report"
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-menubar": "^1.1.16",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toast": "^1.2.15",
    "@radix-ui/react-tooltip": "^1.2.8",
    "class-variance-authority": "^0.7.1",
    "date-fns": "^3.6.0",
    "firebase": "^12.2.1",
    "firebase-admin": "^12.6.0",
    "i18next": "^25.4.0",
    "i18next-icu": "^2.4.0",
    "immer": "^10.1.3",
    "lucide-react": "^0.263.1",
    "merge2": "^1.4.1",
    "next": "^14.2.32",
    "next-themes": "^0.3.0",
    "pako": "^2.1.0",
    "patch-package": "^8.0.0",
    "react": "^18.3.1",
    "react-day-picker": "^8.10.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.54.2",
    "react-hot-toast": "^2.5.2",
    "react-i18next": "^15.7.1",
    "recharts": "^2.15.1",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.0.1",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.24.2",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@napi-rs/canvas": "^0.1.80",
    "@testing-library/jest-dom": "^6.9.0",
    "@testing-library/react": "^16.3.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.16.5",
    "@types/pixelmatch": "^5.2.6",
    "@types/pngjs": "^6.0.5",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^8.39.1",
    "@typescript-eslint/parser": "^8.39.1",
    "autoprefixer": "^10.4.0",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "jest": "^30.2.0",
    "pixelmatch": "^7.1.0",
    "pngjs": "^7.0.0",
    "postcss": "^8.4.0",
    "tailwindcss": "^3.4.0",
    "ts-jest": "^29.4.4",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.39.1"
  }
}
